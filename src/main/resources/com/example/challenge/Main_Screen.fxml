<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="560.0" prefWidth="300.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.challenge.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <BorderPane prefHeight="560.0" prefWidth="300.0">
        <center>
            <AnchorPane prefHeight="485.0" prefWidth="263.0" BorderPane.alignment="CENTER">
            <children>
                    <ImageView fx:id="logo" fitHeight="17.0" fitWidth="115.0" layoutX="92.0" layoutY="363.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@Images/Logo.png" />
                        </image>
                    </ImageView>
            </children></AnchorPane>
        </center>
        <top>
            <AnchorPane prefHeight="123.0" prefWidth="286.0" BorderPane.alignment="CENTER">
                <children>
                    <ImageView fx:id="icon" fitHeight="36.0" fitWidth="34.0" layoutX="252.0" layoutY="5.0" onMouseClicked="#onClickIcon" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@Images/AccountIcon.png" />
                        </image>
                    </ImageView>
                    <Text fx:id="usernamelabel" layoutX="226.0" layoutY="26.0" strokeType="OUTSIDE" strokeWidth="0.0" text="error" />
                </children>
            </AnchorPane>
        </top>
    </BorderPane>

    <Label fx:id="welcomeText" />
</VBox>
